" Powerline for Vim
set rtp+=/Library/Frameworks/Python.framework/Versions/2.7/lib/python2.7/site-packages/powerline/bindings/vim/
set laststatus=2
set t_Co=256

syntax on
set number
set hlsearch
set autoindent
set ruler
set showmode
set tabstop=4

" highlight Comment ctermfg=cyan
highlight Search term=reverse ctermbg=4 ctermfg=7